{
	"name": "react-template",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"prepare": "husky install",
		"dev": "vite",
		"build": "tsc -b && vite build",
		"lint": "eslint .",
		"format": "prettier --write .",
		"preview": "vite preview"
	},
