		"eslint-plugin-react-refresh": "^0.4.26",
		"eslint-plugin-spellcheck": "^0.0.20",
		"globals": "^16.5.0",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.7",
		"prettier": "^3.7.4",
		"tw-animate-css": "^1.4.0",
		"typescript": "~5.9.3",
		"typescript-eslint": "^8.50.1",
		"vite": "^7.2.4"
	}
